@{
    ViewData["Title"] = "Not Found";
}

<section class="not-found">
    <div class="container">
        <img src="~/images/error/404.svg" alt="Error Not Found (404)" />

        <div class="not-found-content">
            <span class="display-5">Ooops!</span>
            <p>The page you are looking for is not available.</p>
            <a class="btn btn-theme" asp-action="Home" asp-controller="Default"><i class="fa-regular fa-house"></i> Go to homepage</a>
        </div>
    </div>
</section>